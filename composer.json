{
    "name": "cable8mm/xeed",
    "description": "The Xeed is to generate a new seed file based on data from the existing database table.",
    "keywords": [
        "seed",
        "laravel",
        "generator",
        "xeed"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Lee",
            "email": "cable8mm@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Cable8mm\\Xeed\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cable8mm\\Xeed\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "./vendor/bin/phpunit tests",
        "lint": "./vendor/bin/pint",
        "inspect": "./vendor/bin/pint --test"
    }
}